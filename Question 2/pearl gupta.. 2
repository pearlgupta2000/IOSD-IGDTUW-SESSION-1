#include <iostream>
using namespace std;
int main()
{
    int t;  cin>>t;
    while(t!=0){
        int n;  cin>>n;
        int arr[n]; int i=0;
        for(i=0;i<n;i++){  cin>>arr[i];  }
        int sum=0;
        i=0;
        for(i=0;i<n;i++){
            sum=sum+arr[i];
        }
        cout<<sum;
        t--;
    }
    return 0;
}
