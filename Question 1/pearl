#include <iostream>
using namespace std;
int main()
{  int t; cin>>t;
  while(t!=0){
      int n,i=0;   cin>>n;  int arr[n];
      for(i=0;i<n;i++){
            cin>>arr[i];
      }
      int sum=0;
      i=0;
      for(i=0;i<n;i++){
          sum=sum+arr[i];
      }
      cout<<sum;
      t--;
  }

    return 0;
}
